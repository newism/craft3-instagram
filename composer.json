{
    "name": "newism/craft3-instagram",
    "description": "Instagram plugin to connect users / site to the Instagram Basic API",
    "type": "craft-plugin",
    "version": "0.0.1",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "instagram"
    ],
    "support": {
        "docs": "https://github.com/newism/instagram/blob/master/README.md",
        "issues": "https://github.com/newism/instagram/issues"
    },
    "license": "proprietary",
    "authors": [
        {
            "name": "Leevi Graham",
            "homepage": "https://newism.com.au"
        }
    ],
    "require": {
        "craftcms/cms": "^3.4",
        "league/oauth2-client": "^2.4",
        "league/oauth2-instagram": "^3.0"
    },
    "require-dev": {
      "craftcms/cms": "^3.4",
      "codeception/codeception": "^3.0",
      "vlucas/phpdotenv": "^3.0"
    },
    "autoload": {
        "psr-4": {
          "newism\\instagram\\": "src/"
        }
    },
    "extra": {
        "name": "Instagram",
        "handle": "instagram",
        "developer": "Leevi Graham",
        "developerUrl": "https://newism.com.au",
        "documentationUrl": "https://github.com/newism/instagram/blob/master/README.md",
        "changelogUrl": "https://raw.githubusercontent.com/newism/instagram/master/CHANGELOG.md",
        "components": {
            "instagram": "newism\\instagram\\services\\Instagram"
        },
        "class": "newism\\instagram\\Plugin"
    }
}
